{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "console_title_template": "{{if .Root}} ‚ö° {{end}}{{.Folder | replace \"~\" \"üèö\" }} @ {{.HostName}}",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "leading_diamond": "\ue0b6",
          "template": "ü¶Ö",
          "foreground": "#ffffff",
          "background": "#863892",
          "type": "os",
          "style": "diamond"
        },
        {
          "template": " \uf0f0 {{ .UserName }} ",
          "foreground": "#ffffff",
          "background": "#40189c",
          "type": "session",
          "style": "diamond"
        },
        {
          "properties": {
            "folder_icon": "\uf07b",
            "folder_separator_icon": " \uf061 ",
            "home_icon": "\ueb06",
            "style": "agnoster_full"
          },
          "leading_diamond": "<#01579B>\ue0b0",
          "trailing_diamond": "\ue0b0</>",
          "template": " {{ .Path }} ",
          "foreground": "#ffffff",
          "background": "#01579B",
          "type": "path",
          "style": "diamond"
        },
        {
          "properties": {
            "branch_icon": "",
            "fetch_stash_count": true,
            "fetch_status": true,
            "fetch_upstream_icon": true,
            "fetch_worktree_count": true
          },
          "template": " {{ .HEAD }}{{ if .Staging.Changed }}<#FF6F00> \uf046 {{ .Staging.String }}</>{{ end }}{{ if and (.Working.Changed) (.Staging.Changed) }} |{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "foreground": "#000000",
          "powerline_symbol": "\ue0b0",
          "background": "#00C853",
          "type": "git",
          "style": "powerline",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}#FFEB3B{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}#FFCC80{{ end }}",
            "{{ if gt .Ahead 0 }}#B388FF{{ end }}",
            "{{ if gt .Behind 0 }}#B388FF{{ end }}"
          ]
        },
        {
          "template": " \ue753 {{ if .Error }}<#FE4A49>?</>{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#000000",
          "type": "angular",
          "style": "powerline"
        },
        {
          "template": " \ue718 {{ .Full }} ",
          "foreground": "#45bf17",
          "powerline_symbol": "\ue0b0",
          "background": "#000000",
          "type": "node",
          "style": "powerline"
        },
        {
          "template": "\ue71e {{ .Full }} ",
          "foreground": "#FE4A49",
          "powerline_symbol": "\ue0b0",
          "background": "#000000",
          "type": "npm",
          "style": "powerline"
        },
        {
          "template": " \ue738 {{ .Full }}",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#000000",
          "type": "java",
          "style": "powerline"
        },
        {
          "template": "<transparent> \uf12a</> {{ reason .Code }} ",
          "foreground": "#ffffff",
          "powerline_symbol": "\ue0b0",
          "background": "#910000",
          "type": "status",
          "style": "powerline"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "style": "dallas",
            "threshold": 0
          },
          "leading_diamond": "\ue0b6",
          "template": "\ueba2 {{ .FormattedMs }}s ",
          "foreground": "#ffffff",
          "background": "#29315A",
          "type": "executiontime",
          "style": "diamond"
        },
        {
          "properties": {
            "charged_icon": "\ue22f ",
            "charging_icon": "\ue234 ",
            "discharging_icon": "\ue231 "
          },
          "trailing_diamond": "| ",
          "template": " {{ if not .Error }}{{ .Icon }}{{ .Percentage }}{{ end }}\uf295 ",
          "foreground": "#ffffff",
          "background": "#29315A",
          "type": "battery",
          "style": "powerline",
          "background_templates": [
            "{{if eq \"Charging\" .State.String}}#40c4ff{{end}}",
            "{{if eq \"Discharging\" .State.String}}#ff5722{{end}}",
            "{{if eq \"Full\" .State.String}}#4caf50{{end}}"
          ]
        },
        {
          "template": " \uf308 {{ .Context }} ",
          "foreground": "#ffffff",
          "background": "#29315A",
          "type": "docker",
          "style": "diamond"
        },
        {
          "template": " \udb84\udcfe {{.Context}}{{if .Namespace}} \uf061 {{.Namespace}}{{end}} ",
          "foreground": "#ffffff",
          "background": "#29315A",
          "type": "kubectl",
          "style": "diamond"
        },
        {
          "properties": {
            "time_format": "15:04:05 | Mon"
          },
          "template": "<transparent> \ue0ba\ue0bc </>{{ .CurrentDate | date .Format }} ",
          "foreground": "#3EC669",
          "background": "#29315A",
          "type": "time",
          "style": "plain"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "properties": {
            "always_enabled": true
          },
          "template": "‚ùØ ",
          "foreground": "#ffffff",
          "type": "status",
          "style": "plain",
          "foreground_templates": [
            "{{ if gt .Code 0 }}#ff0000{{ end }}"
          ]
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
